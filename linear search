#include<stdio.h>
int main()
{
	int a[20],n,i,s;
	printf("enter the number of elements=");
	scanf("%d",&n);
	
	
	printf("enter the number of array elements=");
	for(i=0;i<n;++i)
	scanf("%d",&a[i]);
	  
	printf("enter the element to be searched=");
	scanf("%d",&s);
	
	for(i=0;i<n;i++)
	 if(a[i]==s)=
	 break;
	 	
	 	if(i<n)
		 printf("elements to be found %d",i);
	 	else
		 printf("elements not found");
		 
		 
	return 0;	
  }
